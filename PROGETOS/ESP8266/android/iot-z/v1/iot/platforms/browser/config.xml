<?xml version='1.0' encoding='utf-8'?>
<widget id="iot.zanetti.br" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Badge">
        <param name="browser-package" value="Badge" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Iot-Z</name>
    <description>
        aplicatico voltado para internet das coisas
    </description>
    <author email="gabrielzanetti2012@gmail.com" href="http://www.zanettiautomarketing.com.br">
        Gabriel lorencao Zanetti
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>

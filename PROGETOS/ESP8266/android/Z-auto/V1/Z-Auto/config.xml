<?xml version='1.0' encoding='utf-8'?>
<widget id="zm.br.zanetti" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Z-Auto</name>
    <description>
        aplicativo da empresa zanettiautomarketing para automacao residencial e industrial
    </description>
    <author email="gabrielzanetti2012@gmail.com" href="http://zanettiautomarketing.com.br">
        gabriel lorencao zanetti
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
         <icon density="ldpi" src="icon/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="icon/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="icon/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="icon/icon/android/icon-96-xhdpi.png" />

    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin platform="android" name="cordova-plugin-autostart" version="2.3.0" source="npm"/>

</widget>
